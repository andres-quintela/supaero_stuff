#include <stdio.h>

int incr(int i) {
    return i + 1;
}

void pretty_print(int i){
    printf("Here is %d\n", i);
}

double add(int i, int j) {
    printf("%d\n", i + j);
}

int main(void) {
    int    i = 1;
    double j = 3.5;
    double    z = 0;

    pretty_print(i);

    printf("incr(%d) = %d\n", i, incr(i));

    printf("Now, an addition!\n");

    z = add(i, j);

    return z;
}
