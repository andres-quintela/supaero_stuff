#include <stdbool.h>
#include <stdio.h>


void multiply(int size, float array[size]){
		
		for (int i = 0;i< size ;i++){
				array[i]=array[i]*2;
		}
}

void print(int size, float array[size]){
		
		for (int i = 0;i< size ;i++){
				printf("%f\n",array[i]);
		}
}


int main(void){
	
	
	float matrix[4] = {1.0, 2.0, 3.0, 4.0};

	print(4,matrix);
	
    
    multiply(4,matrix);
    
    print(4,matrix);

                           
            
	return 0;
}
