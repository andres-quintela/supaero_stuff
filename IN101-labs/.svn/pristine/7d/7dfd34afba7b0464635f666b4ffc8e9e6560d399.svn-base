GCC = gcc
CFLAGS = -g -std=c99 -Wall

app-linked-list-int: linked_list_int.c
	$(GCC) $(CFLAGS) -o app-linked-list-int linked_list_int.c

run: app-linked-list-int
	./app-linked-list-int

app-linked-list-int-fun: linked_list_int_fun.c
	$(GCC) $(CFLAGS) -o app-linked-list-int-fun linked_list_int_fun.c

run-fun: app-linked-list-int-fun
	./app-linked-list-int-fun
